{
  "name": "typescript-react-redux-toolkit-boilerplate",
  "version": "1.0.0",
  "description": "Typescript + React + Redux Toolkit boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development",
    "build-test": "DEPLOY=true NODE_ENV='development' webpack --config webpack.config.js --mode development",
    "build-prod": "NODE_ENV='production' webpack --config webpack.config.js --mode production",
    "t": "node ./scripts/translations/order.js && node ./scripts/translations/generate.js",
    "ut": "node ./scripts/translations/unused.js",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renderforest/video-editor.git"
  },
  "engines": {
    "node": "14.17.5",
    "npm": "6.14.8"
  },
  "author": "Tigran Balayan",
  "license": "ISC",
  "browserslist": [
    "> 1%",
    "ie > 9"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^2.4.0",
    "framer-motion": "^11.12.0",
    "i18next": "^23.16.8",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.1.3",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.1",
    "redux": "^5.0.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@svgr/webpack": "^8.1.0",
    "@types/i18next": "^13.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.10.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-i18next": "^8.1.0",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.24",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-perf": "^3.3.3",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "path": "0.12.7",
    "prettier": "^3.4.1",
    "sass": "^1.81.1",
    "sass-loader": "^16.0.3",
    "shelljs": "^0.8.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-scss": "^6.10.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  }
}
